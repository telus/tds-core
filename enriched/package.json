{
  "name": "telus-thorium-enriched",
  "version": "0.4.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run deploy && npm run dev",
    "clean": "rm -rf lib && rm -rf dist && mkdir -p lib && mkdir -p dist",
    "dev": "node devServer.js",
    "pro": "rm -rf assets && webpack --config webpack.config.pro.js",
    "deploy": "rm -rf node_modules && npm install && npm run build",
    "lint": "eslint components examples --ext '.js,.jsx'",
    "build:css": "npm-sass src/index.scss | postcss --use autoprefixer | cleancss > dist/thorium-enriched.min.css",
    "build:lib": "BABEL_ENV=cjs babel ./src -d lib",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:umd:min": "webpack -p --config webpack.config.umd.min.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min && npm run build:css",
    "test": "jest"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "telus-thorium-core": "^0.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "bundle-loader": "^0.5.0",
    "clean-css": "^3.4.20",
    "css-loader": "^0.15.6",
    "enzyme": "^2.4.1",
    "eslint": "^1.1.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "highlight.js": "^9.0.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "jest": "^16.0.1",
    "js-dom": "0.0.1",
    "karma": "^1.3.0",
    "node-sass": "^3.4.2",
    "npm-sass": "^1.3.0",
    "postcss-cli": "^2.6.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.0"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component"
  ],
  "description": "",
  "author": "",
  "sass": "src/index.scss"
}
