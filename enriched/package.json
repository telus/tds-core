{
  "name": "telus-thorium-enriched",
  "version": "0.13.2",
  "main": "lib/index.js",
  "scripts": {
    "preinstall": "scripts/init-npm.sh",
    "start": "npm run deploy && npm run dev",
    "clean": "rimraf lib && rimraf dist && mkdirp lib && mkdirp dist",
    "dev": "node devServer.js",
    "pro": "rimraf assets && webpack --config webpack.config.pro.js",
    "deploy": "rimraf node_modules && npm install && npm run build",
    "lint": "eslint src --ext '.js,.jsx'",
    "build:lib": "BABEL_ENV=cjs babel ./src -d lib --copy-files --ignore ./src/core-styles.js,__tests__",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:umd:min": "webpack -p --config webpack.config.umd.min.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "test": "jest"
  },
  "dependencies": {
    "@telusdigital/redux-contentful": "github:telusdigital/redux-contentful",
    "classnames": "^2.1.3"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "telus-thorium-core": "^0.13.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "bundle-loader": "^0.5.0",
    "clean-css": "^3.4.20",
    "css-loader": "^0.27.3",
    "enzyme": "^2.4.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^1.1.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.3.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.9.0",
    "highlight.js": "^9.0.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "jest": "^16.0.1",
    "js-dom": "0.0.1",
    "karma": "^1.3.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "npm-sass": "^1.3.0",
    "postcss-cli": "^2.6.0",
    "postcss-loader": "^1.0.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.12.3",
    "telus-thorium-core": "^0.13.2",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.2"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component"
  ],
  "description": "TELUS Design System re-usable components",
  "author": "TELUS Digital",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telusdigital/telus-thorium-core.git"
  },
  "license": "UNLICENSED",
  "jest": {
    "rootDir": "./src",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../__mocks__/fileMock.js",
      "^.+\\.(css|less|scss|sass)$": "<rootDir>/../__mocks__/styleMock.js"
    }
  }
}
